[Unit]
Description=ChefOfTheDay Kochdienst-Erinnerungen senden
After=mysql.service

[Service]
Type=oneshot
User=www-data
WorkingDirectory=/home/florian/ChefOfTheDay
ExecStart=/home/florian/ChefOfTheDay/bin/cron-wrapper.sh app:send-reminders 3
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
